#!/bin/bash

docker build \
-t  deskavaenkelt/web_site_demo_ec:v.1 \
--build-arg WEBAPP_PROJECT_ID=WEBAPP_PROJECT_ID \
--build-arg WEBAPP_PRIVATE_KEY_ID=WEBAPP_PRIVATE_KEY_ID \
--build-arg WEBAPP_PRIVATE_KEY=WEBAPP_PRIVATE_KEY \
--build-arg WEBAPP_CLIENT_EMAIL=WEBAPP_CLIENT_EMAIL \
--build-arg WEBAPP_CLIENT_ID=WEBAPP_CLIENT_ID \
--build-arg WEBAPP_CLIENT_X509_CERT_URL=WEBAPP_CLIENT_X509_CERT_URL .
